   "source": [
    "# A，B的交集元素数量/A，B并集的元素数量"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def jaccard_similarity(setA,setB):\n",
    "    return np.intersect1d(setA,setB).shape[0]/np.union1d(setA,setB).shape[0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "A,B = np.arange(10),np.arange(5,12)"
   ]
